#:import statusbar statusbar
<StatusBar>:
    msg_label: _msg_label
    orientation: 'horizontal'
    Label:
        text: 'Total Figures: ' + str(root.counter)
    Label:
        text: 'Selected Figures: ' + str(root.selected_counter) 
    Label:
        id: _msg_label
        text: "Kivy started"
